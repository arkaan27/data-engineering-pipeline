name: uploading_data
conda_env: conda.yml

entry_points:
  main:
    parameters:
      bucket_path:
        description: the url of the bucket
        type: uri
        default: s3://data-engineering-pipeline/FHIR-data

      output_artifact:
        description: Name for the W&B artifact that will be created
        type: string
        default: raw_data

      output_type:
        description: Type of the artifact to create
        type: string
        default: data_upload

      output_description:
        description: Description of the Artifact for reference
        type: string

    command: >-
        python run.py --bucket_path {bucket_path} --output_artifact {output_artifact}  --output_type {output_type}  --output_description {output_description}