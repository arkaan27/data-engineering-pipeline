name: uploading_data
conda_env: conda.yml

entry_points:
  main:
    parameters:

      bucket_name:
        description: the name of the bucket
        type: string

      dataset_path:
        description: The path to the directory of data to be uploaded
        type: string

    command: >-
        python run.py --bucket_name {bucket_name}  --dataset_path {dataset_path}